[rand] > [print] [note@ Random float in (0,1)]
[rand(1,100)] > [print] [note@ Random int in 1,100]

[array]{
    [rand(1,10)]
    [rand(10,20)]
    [rand(20,30)]
}>[store@arr]>[dump] [note@ Array of random ints]

[len@arr]>[store@arrLen] [note@ Length of array]
[math@arrLen-1]>[store@maxIdx]
[rand(0,maxIdx)]>[store@randIndex] [note@ Zero-based index]
[print@randIndex] [note@ Debug: which index?]
[get(randIndex)@arr]>[print] [note@ Random element from array]
